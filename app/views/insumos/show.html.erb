<div class="page-header">
  <h3><%= I18n.t('.title', default: @insumo.class.model_name.human ) %></h3>
</div>
<dl class="dl-horizontal">
    <dt><strong>Descricao:</strong></dt>
  <dd><%= @insumo.descricao %></dd>
    <dt><strong>Grupo insumo:</strong></dt>
  <dd><%= @insumo.grupo_insumo %></dd>
  </dl>
<div class="form-actions">
  <%= link_to I18n.t('.back', default: t("helpers.links.back")),
              insumos_path, class: 'btn'  %>
  <%= link_to I18n.t('.edit', default: t("helpers.links.edit")),
              edit_insumo_path(@insumo), class: 'btn' %>
  <%= link_to I18n.t('.destroy', default: t("helpers.links.destroy")),
              insumo_path(@insumo),
              method: 'delete',
              data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Tem certeza que deseja remover o registro?')) },
              class: 'btn btn-danger' %>
</div>
