<div class="page-header">
  <h3><%= I18n.t('.title', default: @almoxarife.class.model_name.human ) %></h3>
</div>
<dl class="dl-horizontal">
    <dt><strong>Colaborador:</strong></dt>
  <dd><%= @almoxarife.colaborador %></dd>
    <dt><strong>Centro custo:</strong></dt>
  <dd><%= @almoxarife.centro_custo %></dd>
  </dl>
<div class="form-actions">
  <%= link_to I18n.t('.back', default: t("helpers.links.back")),
              almoxarifes_path, class: 'btn'  %>
  <%= link_to I18n.t('.edit', default: t("helpers.links.edit")),
              edit_almoxarife_path(@almoxarife), class: 'btn' %>
  <%= link_to I18n.t('.destroy', default: t("helpers.links.destroy")),
              almoxarife_path(@almoxarife),
              method: 'delete',
              data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Tem certeza que deseja remover o registro?')) },
              class: 'btn btn-danger' %>
</div>
