<div class="page-header">
  <h3><%= I18n.t('.title', default: @prazo_locacao.class.model_name.human ) %></h3>
</div>
<dl class="dl-horizontal">
    <dt><strong>Descricao:</strong></dt>
  <dd><%= @prazo_locacao.descricao %></dd>
  <dt><strong>Qtd Dias:</strong></dt>
  <dd><%= @prazo_locacao.qtd_dias %></dd>
  </dl>
<div class="form-actions">
  <%= link_to I18n.t('.back', default: t("helpers.links.back")),
              prazo_locacoes_path, class: 'btn'  %>
  <%= link_to I18n.t('.edit', default: t("helpers.links.edit")),
              edit_prazo_locacao_path(@prazo_locacao), class: 'btn' %>
  <%= link_to I18n.t('.destroy', default: t("helpers.links.destroy")),
              prazo_locacao_path(@prazo_locacao),
              method: 'delete',
              data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Tem certeza que deseja remover o registro?')) },
              class: 'btn btn-danger' %>
</div>
