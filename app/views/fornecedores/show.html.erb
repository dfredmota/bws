<div class="page-header">
  <h3><%= I18n.t('.title', default: @fornecedor.class.model_name.human ) %></h3>
</div>
<dl class="dl-horizontal">
    <dt><strong>Razao social:</strong></dt>
  <dd><%= @fornecedor.razao_social %></dd>
  <dt><strong>CNPJ:</strong></dt>
  <dd><%= @fornecedor.cnpj %></dd>
    <dt><strong>Nome fantasia:</strong></dt>
  <dd><%= @fornecedor.nome_fantasia %></dd>
    <dt><strong>Telefone:</strong></dt>
  <dd><%= @fornecedor.telefone %></dd>
    <dt><strong>Endereco:</strong></dt>
  <dd><%= @fornecedor.endereco %></dd>
    <dt><strong>Email:</strong></dt>
  <dd><%= @fornecedor.email %></dd>
  </dl>
<div class="form-actions">
  <%= link_to I18n.t('.back', default: t("helpers.links.back")),
              fornecedores_path, class: 'btn'  %>
  <%= link_to I18n.t('.edit', default: t("helpers.links.edit")),
              edit_fornecedor_path(@fornecedor), class: 'btn' %>
  <%= link_to I18n.t('.destroy', default: t("helpers.links.destroy")),
              fornecedor_path(@fornecedor),
              method: 'delete',
              data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Tem certeza que deseja remover o registro?')) },
              class: 'btn btn-danger' %>
</div>
