<div class="page-header">
  <h3><%= I18n.t('.title', default: @usuarios_mobile.class.model_name.human ) %></h3>
</div>
<dl class="dl-horizontal">
    <dt><strong>Login:</strong></dt>
  <dd><%= @usuarios_mobile.login %></dd>
    <dt><strong>Senha:</strong></dt>
  <dd><%= @usuarios_mobile.senha %></dd>
    <dt><strong>Colaborador:</strong></dt>
  <dd><%= @usuarios_mobile.colaborador.nome %></dd>
    <dt><strong>Super usuario:</strong></dt>
  <dd><%= @usuarios_mobile.super_usuario %></dd>
  </dl>
<div class="form-actions">
  <%= link_to I18n.t('.back', default: t("helpers.links.back")),
              usuarios_mobiles_path, class: 'btn'  %>
  <%= link_to I18n.t('.edit', default: t("helpers.links.edit")),
              edit_usuarios_mobile_path(@usuarios_mobile), class: 'btn' %>
  <%= link_to I18n.t('.destroy', default: t("helpers.links.destroy")),
              usuarios_mobile_path(@usuarios_mobile),
              method: 'delete',
              data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Tem certeza que deseja remover o registro?')) },
              class: 'btn btn-danger' %>
</div>
